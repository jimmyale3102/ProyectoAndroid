<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jimmy.hospitalsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.BeginActivity"
            android:theme="@style/beginTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        android:theme="@style/beginTheme" />
        <activity android:name=".activities.Register_DoctorAct" />
        <activity android:name=".activities.Register_PatientAct" />
        <activity android:name=".activities.Register_Appoinment" />
        <activity
            android:name=".activities.MenuActivity"
            android:theme="@style/MenuTheme" />
    </application>

</manifest>